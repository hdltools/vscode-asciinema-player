{
	"name": "asciinema-player",
	"displayName": "Asciinema Player",
	"description": "Asciinema Player",
	"version": "0.0.1",
	"publisher": "congeal",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCustomEditor:asciinemaPlayer.cast"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/congeal/vscode-asciinema-player.git"
	},
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "asciinemaPlayer.cast",
				"displayName": "Asciinema Player",
				"selector": [
					{
						"filenamePattern": "*.cast"
					}
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0",
		"typescript": "^4.0.2"
	}
}
